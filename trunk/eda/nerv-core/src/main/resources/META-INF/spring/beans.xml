<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<camelContext id="nervDefault" autoStartup="true" xmlns="http://camel.apache.org/schema/spring">
		<route>
			<from uri="direct:nerv" />
			<process ref="headersDecorator" />
			<process ref="channelMapper" />
			<process ref="notificationPusher" />
			<dynamicRouter>
				<method ref="nervDynamicRouter" method="route" />
			</dynamicRouter>
			<to uri="stream:out" />
		</route>
	</camelContext>

</beans>